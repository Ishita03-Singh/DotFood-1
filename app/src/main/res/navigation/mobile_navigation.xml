<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_needy">

    <fragment
        android:id="@+id/navigation_needy"
        android:name="com.rsin.dotfood.fragments.NeedyFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_needy" />

    <fragment
        android:id="@+id/navigation_helpers"
        android:name="com.rsin.dotfood.fragments.HelpersFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_helpers" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.rsin.dotfood.fragments.AccountFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_account" />
</navigation>